{
  "name": "eluhr/yii2-swiper",
  "description": "Yii2 extension for Swiper slider",
  "keywords": [
    "yii",
    "yii2",
    "extension",
    "widget",
    "swiper",
    "slider"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kulikov Roman",
      "email": "flinnraider@yandex.ru"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "bower-asset/swiper": ">=v4.4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.x-dev"
  },
  "autoload": {
    "psr-4": {
      "eluhr\\swiper\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
